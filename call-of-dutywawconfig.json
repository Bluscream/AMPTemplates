        "DisplayName":"Game Mode",
        "Category":"Server Settings",
        "Description":"Sets the game mode. Configure the relevant config file accordingly - 'server.cfg' for Multiplayer, 'server_zm.cfg' for Zombies",
        "Keywords":"game,mode,gamemode",
        "FieldName":"GameMode",
        "InputType":"enum",
        "ParamFieldName":"GameMode",
        "DefaultValue":"t4mp \"{{$FullBaseDir}}\" -dedicated +set key {{ServerKey}} +set fs_game {{Mod}} +exec server.cfg",
        "EnumValues":{
            "t4mp \"{{$FullBaseDir}}\" -dedicated -stdout +set key {{ServerKey}} +set fs_game {{Mod}} +exec server.cfg":"Multiplayer (default)",
            "t4sp \"{{$FullBaseDir}}\" -dedicated -stdout +set zombiemode 1 +set key {{ServerKey}} +set fs_game {{Mod}} +exec server_zm.cfg":"Zombies"
        }
