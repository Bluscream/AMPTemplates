        "FieldName":"modname",
        "InputType":"checkbox",
        "ParamFieldName":"modname",
        "DefaultValue":"",
        "EnumValues":{
            "False":"",
            "True":"?modname={{CustomGamemode}}"
        }
    },
    {
        "DisplayName":"Custom Gamemode",
        "Category":"Chivalry Server Settings",
